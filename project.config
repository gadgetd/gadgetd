[project]
	description = System wide USB gadgets and FunctionFS based services manager
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/gadgetd
[access "refs/for/refs/*"]
	read = group user/gadgetd
	push = group user/gadgetd
	label-Code-Review = -2..+2 group user/gadgetd
	label-Verified = -1..+1 group user/gadgetd
	rebase = group user/gadgetd
	removeReviewer = group user/gadgetd
	submit = group user/gadgetd
[access "refs/heads/*"]
	read = group user/gadgetd
	create = group user/gadgetd
	push = +force group user/gadgetd
	pushMerge = group user/gadgetd
[access "refs/tags/*"]
	push = +force group user/gadgetd
	pushTag = group user/gadgetd
	pushSignedTag = group user/gadgetd
